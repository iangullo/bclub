<%= render ModalComponent.new do %>
	<%= form_for @coach, authenticity_token: true, data:{turbo_frame: "_top"} do |form| %>
		<% @c_fields.form = form %>
		<%= form.fields_for :person do |f| %>
			<% @title.form    = f %>
			<% @p_fields.form = f %>
			<%= render @title %>
			<%= render @c_fields %>
			<%= render @p_fields%>
		<% end %>
		<%= render @submit %>
	<% end %>
<% end %>
