<p id="notice"><%= notice %></p>

<h1>Training Slots</h1>

<table>
  <thead>
    <tr>
      <th>Season</th>
      <th>Location</th>
      <th>Wday</th>
      <th>Start</th>
      <th>Duration</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_slots.each do |training_slot| %>
      <tr>
        <td><%= training_slot.season_id %></td>
        <td><%= training_slot.location_id %></td>
        <td><%= training_slot.wday %></td>
        <td><%= training_slot.start %></td>
        <td><%= training_slot.duration %></td>
        <td><%= training_slot.team_id %></td>
        <td><%= link_to 'Show', training_slot %></td>
        <td><%= link_to 'Edit', edit_training_slot_path(training_slot) %></td>
        <td><%= link_to 'Destroy', training_slot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training Slot', new_training_slot_path %>
